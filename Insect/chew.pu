@startuml Insect/chew
hide footbox

participant ":Insect" as insect
participant "target:Mycelium" as target

-> insect : chewMycelium(target)
activate insect

opt CanChew && CanMove && Idle
    insect -> target : die()
    activate target

    ref over target
        mycelium-death
    end ref

    return
    destroy target
end

<-- insect
deactivate insect

@enduml